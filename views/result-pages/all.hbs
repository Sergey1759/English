<link rel='stylesheet' href='/stylesheets/result-pages/all.css' />

{{>navigation}}
<div class="container">
    {{#each results}}
        <div class="result">
            <div class="date">
                21.01.2023 12:23
            </div>
            <div class="type">
                Type testing: {{this.type}}
            </div>
            <a href="http://localhost:3000/result/{{this._id}}">
                <button type="button" class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800">Show answers</button>
            </a>
        </div>
    {{/each}}
</div>